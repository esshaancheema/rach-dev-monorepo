<svg width="800" height="600" viewBox="0 0 800 600" xmlns="http://www.w3.org/2000/svg">
  <defs>
    <linearGradient id="grocery-bg" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#059669;stop-opacity:0.9" />
      <stop offset="100%" style="stop-color:#065F46;stop-opacity:0.9" />
    </linearGradient>
  </defs>
  <rect width="800" height="600" fill="url(#grocery-bg)"/>
  
  <!-- Grocery Delivery Elements -->
  <g transform="translate(400,300)">
    <!-- Delivery Van -->
    <rect x="-50" y="-20" width="100" height="50" rx="8" fill="white" stroke="#065F46" stroke-width="3"/>
    <rect x="-40" y="-10" width="80" height="30" fill="#F0FDF4"/>
    <text x="0" y="10" text-anchor="middle" fill="#065F46" font-size="16" font-weight="bold">GROCERY</text>
    <circle cx="-30" cy="40" r="10" fill="#065F46"/>
    <circle cx="30" cy="40" r="10" fill="#065F46"/>
    
    <!-- Grocery Items -->
    <g transform="translate(-150,-80)">
      <rect width="60" height="60" rx="8" fill="white" stroke="#059669" stroke-width="2"/>
      <text x="30" y="35" text-anchor="middle" fill="#059669" font-size="28">🥕</text>
    </g>
    
    <g transform="translate(90,-80)">
      <rect width="60" height="60" rx="8" fill="white" stroke="#059669" stroke-width="2"/>
      <text x="30" y="35" text-anchor="middle" fill="#059669" font-size="28">🥛</text>
    </g>
    
    <g transform="translate(-30,-120)">
      <rect width="60" height="60" rx="8" fill="white" stroke="#059669" stroke-width="2"/>
      <text x="30" y="35" text-anchor="middle" fill="#059669" font-size="28">🍎</text>
    </g>
    
    <g transform="translate(-80,80)">
      <rect width="60" height="60" rx="8" fill="white" stroke="#059669" stroke-width="2"/>
      <text x="30" y="35" text-anchor="middle" fill="#059669" font-size="28">🥖</text>
    </g>
    
    <g transform="translate(60,80)">
      <rect width="60" height="60" rx="8" fill="white" stroke="#059669" stroke-width="2"/>
      <text x="30" y="35" text-anchor="middle" fill="#059669" font-size="28">🧀</text>
    </g>
    
    <!-- Shopping Basket -->
    <g transform="translate(0,80)">
      <rect x="-20" y="-15" width="40" height="30" rx="5" fill="white" stroke="#059669" stroke-width="2"/>
      <text x="0" y="5" text-anchor="middle" fill="#059669" font-size="16">🛒</text>
    </g>
    
    <!-- Connection Lines -->
    <path d="M -120,-50 Q -75,-25 -50,0" stroke="white" stroke-width="2" fill="none" opacity="0.7"/>
    <path d="M 120,-50 Q 75,-25 50,0" stroke="white" stroke-width="2" fill="none" opacity="0.7"/>
    <path d="M 0,-90 Q 0,-45 0,-20" stroke="white" stroke-width="2" fill="none" opacity="0.7"/>
  </g>
  
  <text x="400" y="480" text-anchor="middle" fill="white" font-size="36" font-weight="bold">Grocery Delivery Platform</text>
  <text x="400" y="520" text-anchor="middle" fill="white" opacity="0.8" font-size="18">Fresh • Fast • Convenient • Contactless</text>
</svg>