{
  "name": "@zoptal/feature-flag-service",
  "version": "1.0.0",
  "description": "Feature flags and A/B testing service for Zoptal",
  "main": "dist/app.js",
  "scripts": {
    "build": "tsc",
    "dev": "tsx watch src/app.ts",
    "start": "node dist/app.js",
    "start:prod": "NODE_ENV=production node dist/app.js",
    "test": "vitest",
    "test:coverage": "vitest --coverage",
    "lint": "eslint src --ext .ts",
    "type-check": "tsc --noEmit",
    "docker:build": "docker build -t zoptal/feature-flag-service .",
    "docker:run": "docker run -p 3007:3007 zoptal/feature-flag-service"
  },
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "compression": "^1.7.4",
    "morgan": "^1.10.0",
    "joi": "^17.11.0",
    "redis": "^4.6.12",
    "pg": "^8.11.3",
    "uuid": "^9.0.1",
    "lodash": "^4.17.21",
    "date-fns": "^2.30.0",
    "crypto-js": "^4.2.0",
    "jsonwebtoken": "^9.0.2",
    "bcryptjs": "^2.4.3",
    "nodemailer": "^6.9.7",
    "winston": "^3.11.0",
    "rate-limiter-flexible": "^7.2.0",
    "express-rate-limit": "^7.1.5",
    "swagger-ui-express": "^5.0.0",
    "swagger-jsdoc": "^6.2.8",
    "@zoptal/feature-flags": "workspace:*",
    "@zoptal/error-tracking": "workspace:*"
  },
  "devDependencies": {
    "@types/node": "^20.10.5",
    "@types/express": "^4.17.21",
    "@types/cors": "^2.8.17",
    "@types/compression": "^1.7.5",
    "@types/morgan": "^1.9.9",
    "@types/uuid": "^9.0.7",
    "@types/lodash": "^4.14.202",
    "@types/crypto-js": "^4.2.1",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/bcryptjs": "^2.4.6",
    "@types/nodemailer": "^6.4.14",
    "@types/swagger-ui-express": "^4.1.6",
    "@types/swagger-jsdoc": "^6.0.4",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "eslint": "^8.56.0",
    "typescript": "^5.3.3",
    "tsx": "^4.6.2",
    "vitest": "^1.1.0",
    "@vitest/coverage-v8": "^1.1.0"
  },
  "keywords": [
    "feature-flags",
    "a/b-testing",
    "experimentation",
    "microservice",
    "api"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/zoptal/monorepo.git",
    "directory": "services/feature-flag-service"
  },
  "author": "Zoptal Team",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  }
}